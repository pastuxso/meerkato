    "db:seed": "turbo db:seed",
    "db:migrate": "turbo db:migrate"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.3.1",
    "typescript": "^5.6.3"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=20"
  }
}